<?xml version="1.0" encoding="utf-8"?>
<!-- This file is necessary to start the application in Azure or on IIS.  It tells IIS to forward requests to the DNX app. -->
<configuration>
	<system.webServer>
		<handlers>
			<add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
		</handlers>
		<httpPlatform processPath="..\approot\web.cmd" arguments="" stdoutLogEnabled="true" startupTimeLimit="3600"/>
		<security>
			<requestFiltering allowDoubleEscaping="true" />
		</security>
	</system.webServer>
	<system.web>
		<httpRuntime requestValidationMode="2.0" requestPathInvalidCharacters="" relaxedUrlToFileSystemMapping="true"/>
		<pages validateRequest="false" />
	</system.web>
</configuration>
